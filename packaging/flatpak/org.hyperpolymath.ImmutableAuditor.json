{
  "app-id": "org.hyperpolymath.ImmutableAuditor",
  "runtime": "org.kde.Platform",
  "runtime-version": "6.6",
  "sdk": "org.kde.Sdk",
  "command": "immutable-auditor",
  "finish-args": [
    "--share=network",
    "--socket=wayland",
    "--socket=fallback-x11",
    "--device=dri",
    "--talk-name=org.freedesktop.Flatpak",
    "--filesystem=home"
  ],
  "modules": [
    {
      "name": "immutable-auditor",
      "buildsystem": "cmake-ninja",
      "builddir": true,
      "config-opts": [
        "-DCMAKE_BUILD_TYPE=Release"
      ],
      "sources": [
        {
          "type": "dir",
          "path": "../.."
        }
      ]
    }
  ]
}
